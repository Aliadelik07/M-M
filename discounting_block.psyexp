        <Param name="Begin Experiment" val="total = 0&amp;#10;block = 0&amp;#10;nk = 0&amp;#10;&amp;#10;short_reward = [4,4,4,4]&amp;#10;short_delay = [1,1,1,1]&amp;#10;&amp;#10;long_reward = [8, 8, 8, 8]&amp;#10;long_delay = [1,15,30,7]&amp;#10;&amp;#10;h = 'start'&amp;#10;n = 0&amp;#10;&amp;#10;a = [0,0,0,0]" valType="extendedCode" updates="constant"/>
        <Param name="Begin JS Experiment" val="total = 0;&amp;#10;block = 0;&amp;#10;short_reward = [1, 7, 8];&amp;#10;short_delay = 1;&amp;#10;long_reward = [4, 28, 32];&amp;#10;long_delay = 5;&amp;#10;h = &quot;start&quot;;&amp;#10;n = 0;&amp;#10;" valType="extendedCode" updates="constant"/>
        <Param name="Begin JS Routine" val="left = 0;&amp;#10;right = 0;&amp;#10;" valType="extendedCode" updates="constant"/>
